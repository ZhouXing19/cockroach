subtest generated_as_identity
statement ok
CREATE TABLE generated_always_t (a INT UNIQUE, b INT GENERATED ALWAYS AS IDENTITY);

statement ok
INSERT INTO generated_always_t (a) VALUES (9), (8), (7);

query II
SELECT * FROM generated_always_t;
----
9  1
8  2
7  3

statement error column "b" cannot be explicitly overridden because it is always generated as an identity column
INSERT INTO generated_always_t (a, b) VALUES (6, 2);

statement ok
CREATE TABLE generated_by_default_t (a INT UNIQUE, b INT GENERATED BY DEFAULT AS IDENTITY);

statement ok
INSERT INTO generated_by_default_t (a) VALUES (9), (8), (7);

query II
SELECT * FROM generated_by_default_t;
----
9  1
8  2
7  3

statement ok
INSERT INTO generated_by_default_t (a, b) VALUES (6, 2);

query II
SELECT * FROM generated_by_default_t;
----
9  1
8  2
7  3
6  2
